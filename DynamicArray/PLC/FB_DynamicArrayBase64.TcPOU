    </Method>
    <Method Name="Clear" Id="{3729fa69-f5b0-44e7-83e0-cc5cbbe6f4c8}">
      <Declaration><![CDATA[METHOD PUBLIC Clear : BOOL
VAR
	_arrayElement : SJsonAIterator;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[_arrayElement := buffer.ArrayBegin(_array);
